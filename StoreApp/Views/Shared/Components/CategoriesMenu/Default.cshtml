@model IEnumerable<Category>
<div class="my-4">
    <ul class="list-group">
        @foreach (var cat in Model)
        {
            <li class="list-group-item">
                <a asp-controller="Category" asp-action="Index" asp-route-id="@cat.CategoryID">@cat.CategoryName</a>
            </li>
        }
    </ul>
</div>